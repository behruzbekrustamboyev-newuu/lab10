#include <iostream>
using namespace std;

bool isIncreasing(int n) {
    if (n < 10) return true;

    int last = n % 10;
    int prev = (n / 10) % 10;

    if (prev >= last) return false;

    return isIncreasing(n / 10);
}

int main() {
    int n;
    cout<< "enter number" ;
    cin >> n;
    if (isIncreasing(n))
        cout << "True";
    else
        cout << "False";
}
